{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": ["**/*.{ts,js}"],
  "entry": [
    "example/**/*.ts",
    "**/*.{config,cfg}.{ts,js}",
    "src/mod.ts",
    "src/**/*-mod.ts",
    "deprecated/mod.ts",
    "deprecated/**/*-mod.ts",
    ".config/**/*.ts",
    ".tests/**/*.ts",
    "e-relifso.ts",
    "e-pathkit.ts",
    "deprecated/**/*.ts",
    "gen-src-mod.ts"
  ],
  "ignore": [
    ".tests/**/*.ts",
    "reliverse.ts",
    "node_modules",
    "dist-jsr",
    "dist-npm",
    "dist-libs",
    "src/types.ts",
    "tests/**/*.ts",
    "deprecated/**/*.ts"
  ],
  "ignoreDependencies": [
    "@babel/plugin-transform-block-scoping",
    "@cspotcode/source-map-support",
    "@rollup/plugin-alias",
    "@rollup/plugin-babel",
    "@rollup/plugin-commonjs",
    "@rollup/plugin-node-resolve",
    "@rollup/plugin-replace",
    "@rollup/plugin-terser",
    "@swc/core",
    "@types/eslint",
    "@types/klaw",
    "@types/minimist",
    "@types/mocha",
    "@types/verror",
    "@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser",
    "@uwx/fsxt-rollup-plugin-dts",
    "eslint-config-google",
    "klaw",
    "minimist",
    "mocha",
    "nyc",
    "rollup",
    "rollup-plugin-bundle-size",
    "rollup-plugin-typescript-paths",
    "ts-node",
    "tsconfig-paths",
    "tslib",
    "typedoc",
    "typedoc-plugin-missing-exports",
    "unplugin-swc",
    "verror",
    "@reliverse/rse"
  ]
}
